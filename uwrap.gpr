with "libadalang";
with "libtemplatelang";
with "libtestlang";

project UWrap is

   for Source_Dirs use ("src");
   for Object_Dir use "obj";
   for Main use ("uwrap");

   package Binder is
      for Switches ("ada") use ("-E");
   end Binder;

   package Compiler is
      for Switches ("ada") use ("-g", "-gnata", "-gnatw.T");
   end Compiler;

end UWrap;
