match Entity () do
    match f_name ("A") and x: prev ()
    weave standard.out ("PREV (\e<f_name>) = \e<x.f_name>\n");

    match r: self (f_name ("B")) do
        match x: next (not f_name ("\e<r.f_name>"))
        weave standard.out ("NEXT NOT MATCH (\e<r.f_name>) = \e<x.f_name>\n");
    end;
end;
