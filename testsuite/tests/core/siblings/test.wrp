match entity () {
    match f_name ("A") and prev (x: true)
    weave with standard.out ("PREV (\e<f_name>) = \e<x.f_name>\n");

    match r: f_name ("B") {
        match next (x: not f_name ("\e<r.f_name>"))
        weave with standard.out ("NEXT NOT MATCH (\e<r.f_name>) = \e<x.f_name>\n");
    }
 
}
