match call () {
    weave with standard.out ("CALL TRAVERSED \e<f_name>\n");

    wrap over;

    match f_name ("^INTO$")
    wrap into;

    match f_name ("^ARGS_THEN_OVER$") {
        wrap all f_args;
        wrap over;
    }
    
    match f_name ("^NESTED_THEN_ARGS_THEN_OVER$") {
        wrap all f_args;
        wrap all f_nested; 
        wrap over;
    }
    
    match f_name ("^ARGS_THEN_NESTED_THEN_OVER$") {
        wrap all f_nested;
        wrap all f_args;
        wrap over;
    }
    
    match f_name ("^ARGS_THEN_INTO$") {
        wrap all f_args;
        wrap into;
    }
}

match entity ()
weave with standard.out ("ENTITY TRAVERSED: \e<f_name>\n");
