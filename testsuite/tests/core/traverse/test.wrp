match Call () {
   weave standard.out ("CALL TRAVERSED \e<f_name>\n");

   wrap over;

   match f_name ("^INTO$")
   wrap into;

   match f_name ("^ARGS_THEN_OVER$") {
      pick f_args.child().all() wrap standard.root ();
      wrap over;
   }
    
   match f_name ("^NESTED_THEN_ARGS_THEN_OVER$") {
      pick f_args.child().all() wrap standard.root ();
      pick f_nested.child().all() wrap standard.root (); 
      wrap over;
   }
    
   match f_name ("^ARGS_THEN_NESTED_THEN_OVER$") {
      pick f_nested.child().all() wrap standard.root ();
      pick f_args.child().all() wrap standard.root ();
      wrap over;
  }
    
   match f_name ("^ARGS_THEN_INTO$") {
      pick f_args.child().all() wrap standard.root ();
      wrap into;
   }
}

match Entity ()
weave standard.out (@ & "ENTITY TRAVERSED: \e<f_name>\n");
