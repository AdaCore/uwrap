match Entity () {
    match f_name ("A(.+)")
    weave standard.out (@ & "\e<f_name>:A-\1\n");

    match f_name ("A(.+)") {
        match f_name ("A(.)(.+)")
        weave standard.out (@ & "\e<f_name>:A-\1-\2\n");
    }

    match f_name ("G(?<suffix>.+)")
    weave standard.out (@ & "\e<f_name>:G-\e<suffix>\n");

    match b: parent (f_name ("F"))
    weave standard.out (@ & "'\e<f_name>' has for parent \e<b.f_name>\n");
}
