template main do
    var a1 : text;
    var a2 : text;
    var a3 : text;
    var a4 : text;
end;

match Entity ()
weave n: main (a2 => @ & "a1 value is '\e<n.a1>'");

match Entity ()
weave n: main (a4 => @ & "a3 value is '\e<n.a3>'");

match main ()
weave (a1 => @ & self.origin.f_name);

match main ()
weave (a3 => @ & origin.f_name);

match main ()
weave standard.out (@ & "\e<a2>\n");

match main ()
weave standard.out (@ & "\e<a4>\n");
