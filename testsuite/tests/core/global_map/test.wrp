template w_Entity do
    var name : text;
end;

var entity_map : map (string, object);

template my_out extends standard.out do
end;

match Entity (x"A(?<a>.+)") do
   match not test.entity_map.filter ("\e<a>") do
      pick test weave (entity_map => @.insert (string ("\e<a>"), new (w_Entity (name => "[\e<a>]"))));
      pick test wrap my_out (@ & "RESULT: " & defer (acc: test.entity_map.filter (v: true).fold ("", acc & v.name & " ")));
   end;
end;
