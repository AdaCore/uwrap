template w_Node do
   var my_set : set (object);
   var my_vector : vector (object);
end;

template visit1 pick origin do
   match s: self() and f_name (x"B(.*)") and parent (wrapper (w: w_Node ()))
   pick w weave (
      my_set => @.include (string (s.f_name)),
      my_vector => @.append (string (s.f_name)));
end;

template visit2 pick origin do
   match s: self() and f_name (x"B(.*)") and parent (wrapper (w: w_Node ()))
   pick w weave (
      my_set => @.include (string (s.f_name)),
      my_vector => @.append (string (s.f_name)));
end;

match Entity ()
wrap visit1 ();

match Entity ()
wrap visit2 ();

match f_name ("A")
wrap w: w_Node ();

match w_Node ()
weave standard.out (@ & "SET: " & self.my_set.element (e: true).fold (c: "", c: (c & e), c: (c & ", ")) & "\n");

match w_Node ()
weave standard.out (@ & "VECTOR: " & self.my_vector.element (e: true).fold (c: "", c: (c & e), c: (c & ", ")) & "\n");