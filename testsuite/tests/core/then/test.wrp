do
   do
      match Entity ("A")
      weave s1: standard.out (@ & "A NAME: \e<f_name>\n");
      else match Entity () weave s1: standard.out (@ & "!A NAME: \e<f_name>\n");
   then
      match Entity ()
      pick s1 weave (@ & "A or !A THEN NAME: \e<self.origin.f_name>\n"); 
   end;
then
   match Entity ("B")
   do 
      weave standard.out (@ & "B NAME (1): \e<f_name>\n"); 
   then
      weave standard.out (@ & "B NAME (2): \e<f_name>\n");
   end;
end;
