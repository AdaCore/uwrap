match Entity () {
    match not parent (Entity ())
    wrap standard.out ("NO PARENT: \e<f_name>\n");

    match parent (Entity ())
    wrap standard.out ("AT LEAST ONE PARENT: \e<f_name>\n"); 

    match parent (Entity () and parent (Entity ()))
    wrap standard.out ("AT LEAST TWO PARENTS: \e<f_name>\n");
}
