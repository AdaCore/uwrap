match entity () {
    match not parent (entity ())
    wrap with standard.out ("NO PARENT: \e<f_name>\n");

    match parent (entity ())
    wrap with standard.out ("AT LEAST ONE PARENT: \e<f_name>\n"); 

    match parent (entity () and parent (entity ()))
    wrap with standard.out ("AT LEAST TWO PARENTS: \e<f_name>\n");
}
